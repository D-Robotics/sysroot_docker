prefix=/usr
exec_prefix=${prefix}
libdir=${prefix}/lib/aarch64-linux-gnu
includedir=${prefix}/include

Name: libbsd
Description: Utility functions from BSD systems (overlay)
Version: 0.11.5
URL: https://libbsd.freedesktop.org/
Libs: -L${libdir} -lbsd
Libs.private:   -lmd
Cflags: -isystem ${includedir}/bsd -DLIBBSD_OVERLAY
